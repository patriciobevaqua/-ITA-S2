{
  "name": "bootstrap5ita",
  "version": "1.0.0",
  "description": "S2 ejercicios",
  "main": "index.js",
  "watch": {
    "sass": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "scss"
      ],
      "quiet": true
    }
  },
  "scripts": {
    "start": "npm-watch",
    "sass": "node-sass --include-path scss ./src/scss/style.scss ./src/style.css",
    "sass-watch": "npm-watch sass",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Patricio Bevaqua",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "node-sass": "^7.0.1",
    "npm-watch": "^0.11.0"
  }
}
